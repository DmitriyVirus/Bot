{
  "version": 2,
  "builds": [
    { "src": "api/bot.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/api/bot", "dest": "/api/bot.py" },
    { "src": "/favicon.ico", "dest": "/static/favicon.ico" },
    { "src": "/favicon_quiz.ico", "dest": "/Quiz/favicon_quiz.ico" },
    { "src": "/(.*)", "dest": "/api/bot.py" },
    { "src": "/send_reminder1", "dest": "/api/bot.py" }
  ]
}
