{
  "version": 2,
  "builds": [
    { "src": "api/bot.py", "use": "@vercel/python" },
    { "src": "webapp/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/bot", "dest": "/api/bot.py" },
    { "src": "/send_reminder1", "dest": "/api/bot.py" },

    { "src": "/google_tab", "dest": "/webapp/google_tab.html" },
    { "src": "/admins", "dest": "/webapp/admins.html" },
    { "src": "/permissions", "dest": "/webapp/permissions.html" },

    { "src": "/webapp/(.*)", "dest": "/webapp/$1" },
    { "src": "/(.*)", "dest": "/api/bot.py" }
  ]
}
